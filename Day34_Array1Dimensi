package Day34_Array1Dimensi;

import java.util.Scanner;

/**
 *
 * @author HP
 */
public class Day34_Array1Dimensi {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
            Scanner serli = new Scanner(System.in);
        System.out.println("masukkan ukuran array: ");
        int a =serli.nextInt();
        String[]nama = new String[a];
        
        for (int i = 0; i < a; i++) {
            nama[i]=serli.nextLine();
            
        }
        System.out.print("{");
          for (int i = 0; i < 5; i++) {
              System.out.print("nama " +nama[i]);
              
        }
          System.out.print("}");
        }
            
    }

